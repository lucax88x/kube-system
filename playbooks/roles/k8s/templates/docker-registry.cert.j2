---
apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: "{{ domains.registry }}"
  namespace: registry
spec:
  secretName: "{{ domains.registry }}"
  issuerRef:
    name: letsencrypt-stage
    kind: ClusterIssuer
  commonName: "{{ domains.registry }}"
  dnsNames:
    - "{{ domains.registry }}"
